{
  "json.schemas": [
    {
      "fileMatch": [
        "*.test_list.json"
      ],
      "url": "file://${factoryFolder}/py_pkg/cros/factory/test/test_lists/test_list.schema.json"
    },
    {
      "fileMatch": [
        "py/goofy/*/manifest.json"
      ],
      "url": "https://json.schemastore.org/chrome-manifest"
    }
  ],
  "python.pythonPath": "/usr/bin/python3.6",
  "python.autoComplete.extraPaths": [
    "${factoryFolder}/py_pkg"
  ],
  "python.analysis.extraPaths": [
    "${factoryFolder}/py_pkg"
  ],
  "python.formatting.provider": "yapf",
  "python.sortImports.args": [
    "--settings-path=${factoryFolder}/devtools/vscode/.isort.cfg"
  ]
}