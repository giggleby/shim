  },
  "sdcard_reader": {
    "generic": {
      "eval": {
        "mmc_host": {
          "is_emmc_attached": false
        }
      },
      "keys": [
        "bus_type",
        "pci_class",
        "pci_vendor_id",
        "pci_device_id",
        "pci_revision",
        "pci_subsystem",
        "sdio_vendor_id",
        "sdio_device_id",
        "usb_vendor_id",
        "usb_product_id",
        "usb_bcd_device"
      ]
    }
  },
  "pcie_emmc_storage_bridge": {
    "generic": {
      "eval": {
        "mmc_host": {
          "is_emmc_attached": true
        }
      },
      "keys": [
        "bus_type",
        "pci_class",
        "pci_vendor_id",
        "pci_device_id",
        "pci_revision",
        "pci_subsystem"
      ],
      "expect": {
        "pci_vendor_id": "!re 0x(?!8086)....",
        "bus_type": "pci"
      }
    }
  },
  "pcie_emmc_storage_assembly": {
    "generic": {
      "eval": [
        {
          "mmc_host": {
            "is_emmc_attached": true
          }
        },
        {
          "generic_storage": {}
        }
      ],
      "keys": [
        "bus_type",
        "mmc_hwrev",
        "mmc_manfid",
        "mmc_name",
        "mmc_oemid",
        "mmc_prv",
        "pci_class",
        "pci_device_id",
        "pci_revision",
        "pci_subsystem",
        "pci_vendor_id",
        "sectors",
        "size",
        "type"
      ],
      "expect": {
        "type": "MMC",
        "bus_type": "pci",
        "pci_vendor_id": "!re 0x(?!8086)...."
      }
    }
  }
}
