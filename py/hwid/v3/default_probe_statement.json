    }
  },
  "pmic": {
    "generic": {
      "eval": {
        "concat": {
          "functions": [
            {
              "glob_path": {
                "pathname": "/sys/bus/platform/devices/*-pmic",
                "filename_only": true
              }
            },
            {
              "glob_path": {
                "pathname": "/sys/bus/platform/devices/*.pwrap:*",
                "filename_only": true
              }
            }
          ]
        }
      }
    }
  },
  "storage": {
    "generic": {
      "eval": "generic_storage",
      "keys":[
        "type",
        "sectors",
        "size",
        "mmc_hwrev",
        "mmc_manfid",
        "mmc_oemid",
        "mmc_name",
        "mmc_prv",
        "pci_vendor",
        "pci_device",
        "pci_class",
        "nvme_model",
        "ata_vendor",
        "ata_model",
        "ufs_vendor",
        "ufs_model"
      ]
    }
  },
  "smart_card_reader": {
    "generic": {
      "eval": "smart_card_usb",
      "keys": [
        "idVendor",
        "idProduct",
        "bcdDevice"
      ]
    }
  },
  "stylus": {
    "generic": {
      "eval": {
        "input_device": {
          "device_type": "stylus"
        }
      },
      "expect": {
        "product": "!re (?!0000)....$",
        "vendor": "!re (?!0000)....$"
      },
      "keys": [
        "name",
        "product",
        "vendor"
      ]
    }
  },
  "touchpad": {
    "generic": {
      "eval": {
