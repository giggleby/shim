{
  "inherit": [
    "disable_factory_server.test_list",
    "generic_grt.test_list"
  ],
  "label": "Generic RMA",
  "constants": {
    "phase": "PVT",
    "rma_mode": true
  },
  "definitions": {
    "RMACr50Reset": {
      "inherit": "TestGroup",
      "label": "Cr50 RMA Cleanup",
      "subtests": [
        {
          "inherit": "ExecShell",
          "label": "Clear RMA Unlock State",
          "args": {
            "commands": "gooftool cr50_disable_rma_mode"
          }
        }
      ]
    },
    "RMAFFT": {
      "inherit": "TestGroup",
      "label": "FFT",
      "subtests": [
        "Placeholder"
      ]
    },
    "RMAGRT": {
      "inherit": "TestGroup",
      "label": "GRT (Google Required Tests)",
      "locals": {
        "station_end_upload_report": true,
        "station": "GRT"
      },
      "subtests": [
        "WriteHWID",
        "RMACr50Reset",
        "RMAGRTEnd",
        "AllCheckPoint",
        "RMAGRTFinalize"
      ]
    },
    "RMAGRTEnd": {
      "inherit": "TestGroup",
      "label": "Finish",
      "subtests": [
        "TPMVerifyEK",
        "ClearTPMOwnerRequest",
        "Barrier",
        "RebootStep"
      ]
    },
    "RMAGRTFinalize": {
      "inherit": "GRTFinalize",
      "args": {
        "rma_mode": true,
        "enable_factory_server": false,
        "upload_method": "none",
        "write_protection": "eval! constants.grt.force_write_protect or options.phase == 'PVT'"
      }
    }
  },
  "tests": [
    "RMAFFT",
    "RMAGRT"
  ]
}
