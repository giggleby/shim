rules:
- name: device_info.set_image_id
  evaluate: >
      SetImageId(
          LookupMap(GetPhase(),
                    {'PROTO': 'PROTO_FAKE_HWID',
                     'EVT': 'EVT_FAKE_HWID',
                     'DVT': 'DVT_FAKE_HWID',
